export command="${command} -linedebug"
export command="${command} -debug"
export command="${command} -uvm"
export command="${command} -gui"
export command="${command} -covoverwrite"
export command="${command} -coverage ALL"
export command="${command} -dutinst top_tb/dut/alu_top_module"
export command="${command} -seed RANDOM"
export command="${command} -f compile_files.f"
export command="${command} -uvmhome CDNS-1.2"
export command="${command} +UVM_TESTNAME=stress_write_test"
export command="${command} -clean"
export command="${command} -l test.log"
export command="${command} +DUMPNAME=test.vcd"
